/* miniprogram/pages/compress/index.wxss */

.card-wrap {
  padding: 100rpx 0;
  background-color: #f8e7ed;
}

.card {
  margin: 0 auto;
  height: 500rpx;
  width: 500rpx;
  box-shadow: 0px 0px 5px 2px rgba(0, 0, 0, 0.3);
  box-sizing: border-box;
}

.img-size {
  height: 100%;
  width: 100%;
  display: block;
  margin: 0 auto;
}

.mt {
}

.btn-wrap {
  position: relative;
  margin: 0 auto;
  margin-top: 20rpx;
  display: flex;
  width: 640rpx;
  justify-content: center;
  color: #333;
  font-size: 22rpx;
}

.btn-self button {
  position: absolute;
  right: 0;
  height: 100%;
  width: 30%;
  opacity: 0;
  z-index: 99;
}

.btn-item {
  flex: 1;
  text-align: center;
  padding-top: 46rpx;
}

.icon-base {
  background-repeat: no-repeat;
  background-position: top center;
  background-size: 20px;
}

.icon-home {
  background-image: url(data:image/png;base64,);
}

.icon-down {
  background-image: url(data:image/png;base64,);
}

.icon-share {
  background-image: url(data:image/png;base64,);
}

.ad{
  margin:0 auto;
  width: 90%;
  margin-top: 50rpx;
}
