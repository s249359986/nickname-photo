page { 
  /* padding-top: 10rpx;  */
  background-color: white;
}

.content {
  display: flex;
  flex-wrap: wrap;  
  margin: 0 auto;
  justify-content: center;
  overflow: hidden;
}
.content:after{
    content: '';
    width: 455rpx;    
  }
.img {
  margin-left: 11rpx;
  margin-right: 11rpx;
  margin-bottom: 10rpx;
  width: 220rpx;
  height: 220rpx;    
}
.img-no {
  width: 0rpx;
  height: 1rpx;   
}




.container{
  display: flex;
  flex-direction: column;
  margin:0 auto;
  padding: 16rpx;

}
.item-wrap{
  color: #333;
  display: flex;
  justify-content: space-between;
  align-items: center;  
}
.item-title{
  margin-top: 40rpx;
  margin-bottom: 20rpx;
  font-size: 40rpx;
  font-weight: bolder;
}
.item-more{
  font-size: 26rpx;
}

.scroll-view_H{
  white-space: nowrap;
}
.scroll-view-item{
  height: 200rpx;
}
.scroll-item{
  display: inline-block;
  width: 50%;
  height: 200rpx;
  padding-right: 16rpx;
}
.swiper-item{
  width: 100%;
  height: 300rpx;
}
.photo-title{
  margin-left: 16rpx;
}
